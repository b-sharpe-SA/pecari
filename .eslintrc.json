{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["standard-with-typescript", "prettier"],
    "plugins": ["prettier"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": ["./tsconfig.json"]
    },
    "rules": {
        "prettier/prettier": 0,
        "@typescript-eslint/explicit-function-return-type": 0,
        "@typescript-eslint/no-throw-literal": 0
    }
}
